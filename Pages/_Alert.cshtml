@model crud_webapp.Services.Alert
<!-- Alert -->
<div class="row margin-top-20">
    <div class="col-md-12">
        @{
            if (Model.Message != null)
            {
                <div class=@Model.Style>
                    <strong>@Model.Title</strong> @Model.Message
                </div>
            }
        }
    </div>
</div>
<!-- Alert -->